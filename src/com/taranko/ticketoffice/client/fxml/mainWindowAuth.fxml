<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.taranko.ticketoffice.client.controllers.MainWindowAuthController">
    <children>
        <AnchorPane layoutY="75.0" prefHeight="526.0" prefWidth="1000.0"/>
        <Label layoutX="387.0" layoutY="29.0" text="Train Tickets Booking">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <GridPane hgap="10.0" layoutX="245.0" layoutY="98.0" maxHeight="-Infinity" maxWidth="-Infinity"
                  minHeight="-Infinity" minWidth="-Infinity" prefHeight="274.0" prefWidth="517.0" vgap="10.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="182.0" minWidth="10.0" prefWidth="115.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="264.0" minWidth="10.0" prefWidth="258.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label prefHeight="25.0" prefWidth="51.0" text="Login">
                    <font>
                        <Font size="16.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="loginField" promptText="Enter login" GridPane.columnIndex="1">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </TextField>
                <Label text="Password" GridPane.rowIndex="1">
                    <font>
                        <Font size="16.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Label>
                <PasswordField fx:id="passwordField" promptText="Password" GridPane.columnIndex="1"
                               GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </PasswordField>
                <Button fx:id="authorizeBtn" mnemonicParsing="false"  prefHeight="31.0"
                        prefWidth="224.0" text="Authorize" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <font>
                        <Font size="15.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets left="20.0"/>
                    </GridPane.margin>
                </Button>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </GridPane>
    </children>
</AnchorPane>
